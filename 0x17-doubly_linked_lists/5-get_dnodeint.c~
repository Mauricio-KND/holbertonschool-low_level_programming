#include "lists.h"

/**
 * get_dnodeint_at_index - gets the node in a dlink list at a given index
 * @head: pointer to head of a dlink list
 * @index: index position of the node
 * Return: pointer to the node at index position or NULL if not found
 */
dlistint_t *get_dnodeint_at_index(dlistint_t *head, unsigned int index)
{
	for (; head && index; head = head->next, index--)
		;
	if (head && !index)
		return (head);
	return (NULL);
}
